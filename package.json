{
    "name": "kikd",
    "description": "Continuous Integration and Continuous Deployment for Open Sourced NPM Tooling.",
    "version": "0.0.25",
    "homepage": "https://github.com/RayBenefield/kikd",
    "author": {
        "name": "Raymond Benefield",
        "email": "raymond.benefield@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/RayBenefield/kikd.git"
    },
    "bugs": {
        "url": "https://github.com/RayBenefield/kikd/issues"
    },
    "license": "MIT",
    "bin": {
        "kd": "cli.js"
    },
    "scripts": {
        "kd": "cd src/node_modules/ && babel-node ./kikd/cli.js",
        "prebuild": "npm run clean && mkdir -p dist/kikd && cp LICENSE readme.md dist/kikd && cat package.json | jq 'del(.babel,.devDependencies,.scripts,.config)' --indent 4 > dist/kikd/package.json",
        "build": "yarn run kd b",
        "precommit": "lint-staged -c ./dx/lint-staged.config.js",
        "commitmsg": "commitlint -e $GIT_PARAMS",
        "clean": "rimraf dist",
        "pretty": "prettier src/node_modules/**/*.js --config prettier.config.js --with-node-modules --write",
        "lint": "npm-run-all lint:src lint:tests",
        "lint:src": "eslint src/node_modules --ignore-pattern 'src/node_modules/__tests__' --config ./dx/eslint.config.js",
        "lint:tests": "eslint src/node_modules/__tests__ --config ./dx/eslint.test.config.js",
        "test": "cd src/node_modules && babel-tape-runner __tests__/{,**/}*.js | tap-spec",
        "sloc": "echo 'Lines of source code: ' && sloc src/node_modules/ -f json  --exclude __tests__ | jq '.summary.source'",
        "watch": "nodemon --watch ./src/node_modules --exec npm-run-all test pretty lint sloc",
        "postrewrite": "./dx/rewrite-tags",
        "postversion": "js-beautify -r package.json && PACKAGE_VERSION=$(cat package.json | jq '.version' --raw-output) && git commit -a -m \"docs(version): Release v$PACKAGE_VERSION.\" -m '[skip ci]' && git tag \"v$PACKAGE_VERSION\""
    },
    "dependencies": {
        "es6-promisify": "^5.0.0",
        "find-up": "^2.1.0",
        "glob": "^7.1.2",
        "minimist": "^1.2.0",
        "path-is-inside": "^1.0.2",
        "renderkid": "^2.0.1",
        "rollup": "^0.52.1",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-executable": "^1.1.0",
        "rollup-plugin-filesize": "^1.5.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify": "^2.0.1",
        "transmutation": "^0.0.46",
        "uglify-es": "^3.2.2"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-tape-runner": "^2.0.1",
        "blue-tape": "^1.0.0",
        "commitlint": "^5.2.0",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.3.0",
        "eslint-plugin-jsx-a11y": "^5.0.3",
        "eslint-plugin-prettier": "^2.3.1",
        "eslint-plugin-react": "^7.1.0",
        "husky": "^0.14.3",
        "js-beautify": "^1.6.14",
        "lint-staged": "^6.0.0",
        "node-jq": "^0.7.0",
        "nodemon": "^1.11.0",
        "npm-run-all": "^4.1.2",
        "prettier": "^1.9.1",
        "rimraf": "^2.6.1",
        "sloc": "^0.2.0",
        "tap-spec": "^4.1.1",
        "tape": "^4.8.0",
        "tape-bdd": "^0.0.1"
    }
}