
using. /Fortnite.com/Devices
using. /Verse.org/Simulation
using. /UnrealEngine.com/Temporary/SpatialMath

using. Tableau

column_device<public> := class(tableau_device):
    @editable ResourceID<override>:string = "Column"

    GetResources<override>():[]resource= array:
        resource:
            Type := ResourceID
            Props := map:
                "Blocks" => Strs of array{}
                "Height" => Integer of (Path("Blocks"), "Length")
            Actions := map:
                "Add Block" => Action of array:
                    Execute("Block Generator", "Create Block")
                    Insert("Architect Cursor", "Block ID").Into("Blocks")
