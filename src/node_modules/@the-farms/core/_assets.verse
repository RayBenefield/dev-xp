<#>
    Setters

IDSetter<public><constructor>(Mat:MI_WaterIcon_material)<transacts> := set_device_id_fn. Mat := Mat
set_device_id_fn<public> := class(scalar_setter):
    Mat<public>:MI_WaterIcon_material
    FN<override>(F:float):void= set Mat.DeviceID = F

CreateWaterIconMaterialInstance()<transacts>:material_instance=
    Material := MI_WaterIcon_material{}
    material_instance:
        Material := Material
        ScalarSetterMap := map:
            "DeviceID" => IDSetter(Material)

