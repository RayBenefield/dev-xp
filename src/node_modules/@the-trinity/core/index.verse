
using. /Verse.org/Simulation
using. /Fortnite.com/Devices

using. Tableau

boss_device<public> := class(tableau_device):
    @editable ResourceID<override>:string = "Boss"
    GetResources<override>():[]resource= array. resource:
        Actions := map:
            "Increase Health" => Action of array:
                Increase("Boss Health", "Count").By(ONE())
                Increase("Gold", "Count").By(ONE())
                Log(ONE())
