
using. /Fortnite.com/Devices
using. /Verse.org/Simulation

using. Numb
using. Vertex

commodity_settings<public> := class<concrete>():
    @editable Default:num = num. Value := 0.0

CreateCommodityResource<public><constructor>(ID:string, R:commodity_settings) := resource:
    ID := ID
    Props := map:
        "Count" => Num of R.Default

commodity_device<public> := class(tableau_device):
    @editable ResourceID<override>:string = "Gold"
    @editable Settings<public>:commodity_settings = commodity_settings{}

    GetResources<override>():[]resource= array. CreateCommodityResource(ResourceID, Settings)
