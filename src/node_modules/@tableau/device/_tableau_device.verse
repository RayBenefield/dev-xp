
using. /Verse.org/Simulation
using. /Fortnite.com/Devices

using. Tableau

tableau_device<public> := class(creative_device, config_device, side_effect_device):
    @editable ResourceID<public>:string = "Resource ID"
    @editable PerResourceID<public>:logic = false
    @editable SubResourceID<public>:string = "Player"
    @editable PerSubResourceID<public>:logic = true

    GetResources<override>():[]resource= array{}

    GetResourceID<override>()<computes>:?string= if (not PerResourceID?). option{ ResourceID } else. false
    GetPerType<override>()<computes>:?string= if (PerResourceID?). option{ ResourceID } else. false
    GetPerSubType<override>()<computes>:?string= if (PerSubResourceID?). option{ SubResourceID } else. false
    GetSideEffects<override>(State:scoped_state)<suspends>:[]side_effect= array{}
