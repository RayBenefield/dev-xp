
using. /Fortnite.com/Devices
using. /Verse.org/Simulation

using. Numb
using. Vertex
using. Tableau
using. TableauResource

basic_settings<public> := class<concrete>():
    @editable ID:string = ""
    @editable Default:num = num. Value := 0.0

CreateBasicResource<public><constructor>(R:basic_settings) := resource:
    ID := R.ID
    Props := props:
        Static := map:
            "Name" => prop_str. Default := R.ID
        Per := map:
            "Count" => prop_num. Default := R.Default

basic_config<public> := class(creative_device, config_device):
    @editable BasicSettings<public>:[]basic_settings = array{}

    GetResources<override>():[]resource=
        for (R:BasicSettings). CreateBasicResource(R)
