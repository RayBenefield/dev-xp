using { Tableau }
using { TableauParticipant }

entity_producer<public> := class(entity):
    Name<override>()<computes>:string = "Producer"

ev_producer_added<public> := class(ev):
    Name<override>()<computes>:string = "Producer Added"

ev_producer_purchased<public> := class(ev):
    Name<override>()<computes>:string = "Producer Purchased"

ev_producer_lost<public> := class(ev):
    Name<override>()<computes>:string = "Producer Lost"

Ev_ProducerAdded<public><constructor>(ProducerID:string) := domain_event:
    SubjectType := entity_producer{}
    Subject := ProducerID
    Event := ev_producer_added{}

Ev_ProducerPurchased<public><constructor>(ProducerID:string, ParticipantID:string) := domain_event:
    SubjectType := entity_producer{}
    Subject := ProducerID
    Event := ev_producer_purchased{}
    TargetType := option. entity_participant{}
    Target := ParticipantID
