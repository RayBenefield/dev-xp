using { Tableau }

entity_participant<public> := class(entity):
    Name<override>()<computes>:string = "Participant"

ev_resources_gained<public> := class(ev):
    Name<override>()<computes>:string = "Resources Gained"

Ev_ResourcesGained<constructor><public>(ID:int) := domain_event:
    Entity := entity_participant{}
    EntityID := ID
    Event := ev_resources_gained{}