using { Tableau }
using { TableauResource }

entity_participant<public> := class(entity):
    Name<override>()<computes>:string = "Participant"

ev_participant_added<public> := class(ev):
    Name<override>()<computes>:string = "Participant Added"

ev_resources_gained<public> := class(ev):
    Name<override>()<computes>:string = "Resources Gained"

Ev_ParticipantAdded<constructor><public>(ID:string) := domain_event:
    SubjectType := entity_participant{}
    Subject := ID
    Event := ev_participant_added{}

Ev_ResourcesGained<constructor><public>(ParticipantID:string, ResourceID:string) := domain_event:
    SubjectType := entity_participant{}
    Subject := ParticipantID
    Event := ev_resources_gained{}
    TargetType := option. entity_resource{}
    Target := ResourceID