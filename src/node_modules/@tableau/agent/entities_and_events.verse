using { Tableau }
using { TableauParticipant }

ev_participant_possessed<public> := class(ev):
    Name<override>()<computes>:string = "Participant Possessed"

ev_participant_abandoned<public> := class(ev):
    Name<override>()<computes>:string = "Participant Abandoned"

Ev_ParticipantPossessed<constructor><public>(ID:string) := domain_event:
    SubjectType := entity_participant{}
    Subject := ID
    Event := ev_participant_possessed{}

Ev_ParticipantAbandoned<constructor><public>(ID:string) := domain_event:
    SubjectType := entity_participant{}
    Subject := ID
    Event := ev_participant_abandoned{}
