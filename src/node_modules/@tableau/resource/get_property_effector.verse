using { Tableau }

get_property_effector<public> := class(oldeffector):
    GameState<public>:game_state

    Play<override>(Command:command, Pay:load)<suspends>:?command=
        ResourceID := Command.Subject
        ParticipantID := Command.Target
        PropertyID := Pay.Str

        if (GSResource := GameState.Resources[ResourceID]):
            PropType := GSResource.PropTypes[PropertyID] or prop_type.Unknown
            Prop := GameState.GetNumProp(ResourceID, PropertyID, ParticipantID)
            Value := Prop.Get()

            NewCommand := command:
                CopyCommand<constructor>(Command)
                Pay := load. Num := Value
            return option. NewCommand

        return option. Command
