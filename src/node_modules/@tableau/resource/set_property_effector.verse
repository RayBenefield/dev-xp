using { Tableau }

set_property_effector<public> := class(effector):
    GameState<public>:game_state
    SetPropertyCommand<public>:event(command) = event(command){}

    Play<override>(Command:command, Payload:payload):command=
        SetPropertyCommand.Signal(command:
            CopyCommand<constructor>(Command)
            Property := Payload.Str
        )

        return Command
