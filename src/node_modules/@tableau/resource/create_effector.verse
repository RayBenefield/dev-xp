
using. Tableau

Create<public><constructor>(Focus:string) := effect. Create<constructor>(Path(Focus))
Create<public><constructor>(ResID:string, PropID:string) := effect. Create<constructor>(Path(ResID, PropID))
Create<public><constructor>(ResID:half_path, PropID:string) := effect. Create<constructor>(Path(ResID, PropID))
Create<public><constructor>(ResID:string, PropID:half_path) := effect. Create<constructor>(Path(ResID, PropID))
Create<public><constructor>(ResID:half_path, PropID:half_path) := effect. Create<constructor>(Path(ResID, PropID))
Create<public><constructor>(Focus:half_path) := effect{ FN := "Create Resource", Focus := Focus }

create_effector<public> := class(effector):
    GameState<public>:game_state

    Play<override>(Source:string, Activator:string, Focus:prop_path, Tweak:prop_path)<suspends>:?[]command=
        ResourceID := Focus.ResourceID
        PlayerID := Activator
        PropertyID := Focus.PropertyID
        NewID := GameState.GetStrProp(Focus, PlayerID).Get()
        Type := GameState.GetStrProp(Tweak, PlayerID).Get()
        GameState.CreateResource(NewID, Type)
        return option. array{}
