using { Tableau }
using { TableauParticipant }

entity_resource<public> := class(entity):
    Name<override>()<computes>:string = "Resource"

ev_resource_added<public> := class(ev):
    Name<override>()<computes>:string = "Resource Added"

ev_property_updated<public> := class(ev):
    Name<override>()<computes>:string = "Property Updated"

Ev_ResourceAdded<constructor><public>(ID:string) := domain_event:
    SubjectType := entity_resource{}
    Subject := ID
    Event := ev_resource_added{}

Ev_PropertyUpdated<constructor><public>(ID:string, PropertyID:string, ParticipantID:string) := domain_event:
    Event := ev_property_updated{}
    SubjectType := entity_resource{}
    Subject := ID
    TargetType := option. entity_participant{}
    Target := ParticipantID
    Property := PropertyID
