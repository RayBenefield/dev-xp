using { Tableau }

entity_resource<public> := class(entity):
    Name<override>()<computes>:string = "Resource"

ev_resource_added<public> := class(ev):
    Name<override>()<computes>:string = "Resource Added"

ev_resource_renamed<public> := class(ev):
    Name<override>()<computes>:string = "Resource Renamed"

ev_property_updated<public> := class(ev):
    Name<override>()<computes>:string = "Property Updated"

Ev_ResourceAdded<constructor><public>(ID:int) := domain_event:
    Entity := entity_resource{}
    EntityID := ID
    Event := ev_resource_added{}

Ev_ResourceRenamed<constructor><public>(ID:int) := domain_event:
    Entity := entity_resource{}
    EntityID := ID
    Event := ev_resource_renamed{}

Ev_PropertyUpdated<constructor><public>(ID:int) := domain_event:
    Entity := entity_resource{}
    EntityID := ID
    Event := ev_property_updated{}
