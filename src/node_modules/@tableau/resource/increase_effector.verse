using { Tableau }

increase_effector<public> := class(effector):
    GameState<public>:game_state

    Play<override>(Command:command, Payload:payload):command=
        if (Magnitude := Command.Magnitudes[0]):
            return command:
                CopyCommand<constructor>(Command)
                Magnitudes := array. (Magnitude + Payload.Float)
        return Command
