NormalizeNoise<public>(Value:float)<computes>:float=
    AssumedMin := -0.707
    AssumedMax := 0.707
    (Value - AssumedMin) / (AssumedMax - AssumedMin)
Float_NormalNoise<public><constructor>(Focus:string)<transacts> := prop_float_normal_noise{ Focus := Path(Focus) }
prop_float_normal_noise<public> := class<concrete>(prop_float):
    @editable FN<override>:string = "NormalizeNoise"
    ValueFor<override>(L_:scoped_state):float_goo= FloatGoo of Map(L_.ToFloat(Focus), NormalizeNoise)
