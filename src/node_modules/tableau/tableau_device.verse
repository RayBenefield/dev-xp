
using. /Fortnite.com/Devices
using. /Verse.org/Simulation/Tags

using. TableauResource

tableau_device_tag := class(tag){}
tableau_device<public> := interface:
    Init<public>(State:game_state)<suspends>:void

LoadTableauDevices<public>():[]tableau_device=
    TaggedDevices := GetCreativeObjectsWithTag(tableau_device_tag{})
    for(Index -> Tagged : TaggedDevices, Device := tableau_device[Tagged]). Device

config_device_tag := class(tag){}
config_device<public> := interface:
    GetResources<public>():[]resource

LoadConfigDevices<public>():[]config_device=
    TaggedDevices := GetCreativeObjectsWithTag(config_device_tag{})
    for(Index -> Tagged : TaggedDevices, Device := config_device[Tagged]). Device
