
ZERO<public>()<transacts>:half_path = Path("GLOBAL", "ZERO")
ONE<public>()<transacts>:half_path = Path("GLOBAL", "ONE")
EMPTY<public>()<transacts>:half_path = Path("GLOBAL", "EMPTY")
TRUE<public>()<transacts>:half_path = Path("GLOBAL", "TRUE")
FALSE<public>()<transacts>:half_path = Path("GLOBAL", "FALSE")
COMMA<public>()<transacts>:half_path = Path("GLOBAL", "COMMA")

CreateGlobalsResource<public><constructor>()<transacts> := resource:
    ID := "GLOBAL"
    Props := map:
        "ZERO" => Integer of 0
        "ONE" => Integer of 1
        "EMPTY" => Str of ""
        "TRUE" => Bool of true
        "FALSE" => Bool of false
        "COMMA" => Str of ","
