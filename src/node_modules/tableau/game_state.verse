        Key := "{ResourceID},{PropertyID},{SubID}"
        if (Cached := GatheredBoolsCache[Key]). return Cached

        Scoped := scoped_state{ GameState := Self, Source := ResourceID, PropertyID := PropertyID, PlayerID := SubID }
        IDs := GetStrsProp(ResourceID, IDSource, SubID)
        Result := BoolMapGoo(FlatMap(IDs, Scoped.AwaitBool))
