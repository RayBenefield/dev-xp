effector_type<public> := enum:
    AddResources
    BuyProducer
    Produce

effector_provider<public> := class():
    var Effectors:[effector_type]effector = map{}

    Register<public>(EffectorType:effector_type, Effector:effector):void=
        if (set Effectors[EffectorType] = Effector) {}

    Get<public>(Type:effector_type)<decides><transacts>:effector = Effectors[Type]
