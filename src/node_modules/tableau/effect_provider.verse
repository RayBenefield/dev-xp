
effector_provider<public> := class():
    var Effectors:[string]effector = map{}
    var OldEffectors:[string]oldeffector = map{}

    Register<public>(Type:string, Effector:effector):void=
        if (set Effectors[Type] = Effector) {}

    Get<public>(Type:string)<decides><transacts>:effector = Effectors[Type]

    RegisterOld<public>(Type:string, Effector:oldeffector):void=
        if (set OldEffectors[Type] = Effector) {}

    GetOld<public>(Type:string)<decides><transacts>:oldeffector = OldEffectors[Type]
