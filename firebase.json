{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/auth/**",
        "function": "auth"
      },
      {
        "source": "/callback/**",
        "function": "callback"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
